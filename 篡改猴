// ==UserScript==
// @name         å®šæ—¶éšæœºç‚¹å‡»æŒ‰é’®
// @namespace    http://tampermonkey.net/
// @version      1.0
// @description  åœ¨ç‰¹å®šé¡µé¢ä¸Šå®šæ—¶ç‚¹å‡»æŸä¸ªæŒ‰é’®ï¼ˆå¸¦éšæœºé—´éš”ï¼‰
// @author       You
// @match        *://*.cloudworkstations.dev/*vnc.html*
// @grant        GM_log
// ==/UserScript==

(function () {
    'use strict';

    function randomDelay(min, max) {
        return Math.floor(Math.random() * (max - min + 1)) + min;
    }

    function logWithTime(msg) {
        const now = new Date().toLocaleString();
        console.log(`[${now}] ${msg}`);
    }

    function clickButton() {
        // æŸ¥æ‰¾æŒ‰é’®
        const button = document.querySelector('div[title="æ˜¾ç¤º/éšè—æ§åˆ¶æ "]');

        if (button) {
            logWithTime('ğŸ¯ æ‰¾åˆ°æŒ‰é’®ï¼Œå°è¯•è§¦å‘ mousedown å’Œ mouseup äº‹ä»¶...');

            try {
                button.focus();
                logWithTime('ğŸ” å·²èšç„¦æŒ‰é’®');
                const downEvent = new MouseEvent('mousedown', {
                    bubbles: true,
                    cancelable: true,
                    button: 0
                });
                button.dispatchEvent(downEvent);
                logWithTime('âœ… mousedown äº‹ä»¶å·²æ´¾å‘ã€‚');

                setTimeout(() => {
                    const upEvent = new MouseEvent('mouseup', {
                        bubbles: true,
                        cancelable: true,
                        button: 0
                    });
                    button.dispatchEvent(upEvent);
                    logWithTime('âœ… mouseup äº‹ä»¶å·²æ´¾å‘ã€‚');

                    const clickEvent = new MouseEvent('click', {
                        bubbles: true,
                        cancelable: true,
                        button: 0
                    });
                    button.dispatchEvent(clickEvent);
                    logWithTime('âœ… click äº‹ä»¶ä¹Ÿå·²æ´¾å‘ (åœ¨ mouseup å)ã€‚');

                }, 50);

            } catch (e) {
                logWithTime('âŒ æ´¾å‘äº‹ä»¶æ—¶å‡ºé”™: ' + e);
            }

        } else {
            logWithTime('âš ï¸ æœªæ‰¾åˆ°æŒ‰é’®ã€‚');
        }

        const nextDelay = randomDelay(1 * 60 * 1000, 3 * 60 * 1000); // 1-3 åˆ†é’Ÿ
        logWithTime(`â³ ä¸‹ä¸€æ¬¡ç‚¹å‡»å°†åœ¨ ${(nextDelay / 1000 / 60).toFixed(1)} åˆ†é’Ÿå`);
        setTimeout(clickButton, nextDelay);
    }

    window.addEventListener('load', function () {
        GM_log(`[${new Date().toLocaleString()}] é¡µé¢åŠ è½½å®Œæˆ (window.onload)ï¼Œå°†å¯åŠ¨é¦–æ¬¡ç‚¹å‡»å®šæ—¶å™¨...`);
        const initialDelay = randomDelay(1000, 3000);
        GM_log(`[${new Date().toLocaleString()}] ğŸš€ åˆå§‹å»¶è¿Ÿ ${initialDelay}ms åè¿›è¡Œé¦–æ¬¡ç‚¹å‡»å°è¯•`);
        setTimeout(clickButton, initialDelay);
    });

    GM_log(`[${new Date().toLocaleString()}] noVNC ç‚¹å‡»è„šæœ¬å·²æ³¨å…¥ï¼Œç­‰å¾…é¡µé¢åŠ è½½å®Œæˆ...`);
})();
